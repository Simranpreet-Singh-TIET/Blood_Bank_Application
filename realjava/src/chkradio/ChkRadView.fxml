<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="500.0" minWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chkradio.ChkRadViewController">
   <children>
      <Label layoutX="185.0" layoutY="30.0" text="Fee Details" textFill="RED">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <CheckBox fx:id="chkCpp" layoutX="97.0" layoutY="112.0" mnemonicParsing="false" onAction="#check_c" text="C++ : 6800">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="chkDsa" layoutX="97.0" layoutY="151.0" mnemonicParsing="false" onAction="#check_d" text="DSA : 7600">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="chkJava" layoutX="97.0" layoutY="190.0" mnemonicParsing="false" onAction="#check_j" text="Java : 8500">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </CheckBox>
      <Button layoutX="267.0" layoutY="114.0" mnemonicParsing="false" onAction="#doCalcFee" prefHeight="38.0" prefWidth="153.0" text="Get Total Fee">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Label fx:id="lblResult" layoutX="256.0" layoutY="177.0" prefHeight="27.0" prefWidth="217.0" text="Total Amount Rs.">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <CheckBox fx:id="chkAll" layoutX="61.0" layoutY="76.0" mnemonicParsing="false" onAction="#doChkUncheckAll" text="Select All">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </CheckBox>
      <Separator layoutX="26.0" layoutY="231.0" prefHeight="2.0" prefWidth="442.0" />
   </children>
</AnchorPane>
